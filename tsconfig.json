{
  "compilerOptions": {
                          
    "lib": ["DOM", "DOM.Iterable","ESNext"],                                   
    "allowJs": true,                                                 
    "jsx": "react-jsx",                              
    "noEmit": true,                                      
    "strict": true,                                 
    "moduleResolution": "node",                  
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,        
    "esModuleInterop": true,                        
    "skipLibCheck": true,                           
    "forceConsistentCasingInFileNames": true        
  },
  "include": ["src"]
} 
